		{
			"url": "modules/types/edition/filter_editor/",
			"configuration": {
				"sections": {},
				"groups": {
					"group": [
						{
							"script": [
								"var data = {};\n\nrequire(['lodash'], function(_){\n    var thermCold = hexToRgb('#0000ff');\nvar thermHot = hexToRgb('#ff0000');\n\nfunction hexToRgb(hex) {\n  // Expand shorthand form (e.g. \"03F\") to full form (e.g. \"0033FF\")\n  var shorthandRegex = /^#?([a-f\\d])([a-f\\d])([a-f\\d])$/i;\n  hex = hex.replace(shorthandRegex, function(m, r, g, b) {\n      return r + r + g + g + b + b;\n  });\n\n  var result = /^#?([a-f\\d]{2})([a-f\\d]{2})([a-f\\d]{2})$/i.exec(hex);\n  return result ? {\n      r: parseInt(result[1], 16),\n      g: parseInt(result[2], 16),\n      b: parseInt(result[3], 16)\n  } : null;\n}\n\nfunction makeGradientColor(color1, color2, percent) {\n    var newColor = {};\n\n    function makeChannel(a, b) {\n        return(a + Math.round((b-a)*(percent/100)));\n    }\n\n    function makeColorPiece(num) {\n        num = Math.min(num, 255);   // not more than 255\n        num = Math.max(num, 0);     // not less than 0\n        var str = num.toString(16);\n        if (str.length < 2) {\n            str = \"0\" + str;\n        }\n        return(str);\n    }\n\n    newColor.r = makeChannel(color1.r, color2.r);\n    newColor.g = makeChannel(color1.g, color2.g);\n    newColor.b = makeChannel(color1.b, color2.b);\n    newColor.cssColor = \"#\" + \n                        makeColorPiece(newColor.r) + \n                        makeColorPiece(newColor.g) + \n                        makeColorPiece(newColor.b);\n    return(newColor);\n}\n\nfunction temperatureInPercent(temp) {\n    var tempMin = 20;\n    var tempMax = 35;\n    if(temp < tempMin) {\n        return 0;\n    }\n    else if(temp > tempMax) {\n        return 1;\n    }\n    \n    return (temp-tempMin)/(tempMax-tempMin);\n}\n\nfunction pressureInDegrees(pressure) {\n    var pressMin = 970;\n    var pressMax = 1050;\n    if(pressure < pressMin) {\n        return -90;\n    }\n    else if(pressure > pressMax) {\n        return 90;\n    }\n    return (pressure-pressMin)/(pressMax-pressMin)*180-90;\n}\n\nfunction sunInPercent(sun) {\n    var sunMin = 100;\n    var sunMax = 900;\n    sun = sun<sunMin ? sunMin : sun;\n    sun = sun>sunMax ? sunMax : sun;\n    return 1 - (sun-sunMin)/(sunMax-sunMin);\n}\n\n// Commented part: bug in chrome on concurrent animations\n// Works in firefox and chrome canary\n\n// data['pressure-needle'] = {\n//     animation: [\n//         {\n//             tag: 'animateTransform',\n//             attributes: {\n//                 attributeName: 'transform',\n//                 dur: \"1s\",\n//                 to: \"-30,0,0\",\n//                 fill:'freeze',\n//                 type:'rotate'\n//             }\n//         }    \n//     ]\n// };\n\nvar defaultAnimation = {\n    fill: 'freeze',\n    dur: '1s',\n    options: {\n        clearOnEnd: true,\n        persistOnEnd: true\n    }\n};\n\nvar svgmodifier = [\n    \n    {\n        selector: '#cloud',\n        attributes: {\n            opacity: sunInPercent(value.sun[value.sun.length-1])\n        },\n        animation: _.cloneDeep(defaultAnimation)\n    },\n    {\n        selector: '#sun-group',\n        info: value.sun\n    },\n    // {\n    //     selector: '#pressure-needle',\n    //     attributes: {\n    //         transform: 'rotate(' + pressureInDegrees(value.pressure[value.pressure.length-1]) + ')'\n    //     }\n    // },\n    {\n        selector: '#pressure-needle',\n        animation: {\n            tag: 'animateTransform',\n            attributes: {\n                attributeName: 'transform',\n                dur: \"1s\",\n                to: pressureInDegrees(value.pressure[value.pressure.length-1])+ \",0,0\",\n                fill:'freeze',\n                type:'rotate'\n            },\n            options: {\n                persistOnEnd: false,\n                clearOnEnd: {\n                    timeout: 1100\n                }\n            }\n        }   \n    },\n    {\n        selector:'#humidity-value',\n        innerVal: Math.round(value.humidity[value.humidity.length-1]).toString()+'%'\n    },\n    {\n        selector:'#humidity-level',\n        attributes: {\n            height: 0.3 * value.humidity[value.humidity.length-1],\n        },\n        animation: {\n            dur: '1s',\n            fill: 'freeze',\n            options: {\n                clearOnEnd: {\n                    timeout: 1200\n                },\n                persistOnEnd: false\n            }\n        }\n    },\n    {\n        selector: '#pressure-value',\n        innerVal: Math.round(value.pressure[value.pressure.length-1])\n    },\n    {\n        selector:'#living-room-pressure',\n        info: value.pressure\n    },\n    {\n        selector:'#living-room-thermometer',\n        info: value.temperature\n    },\n    {\n        selector: '#living-room-humidity',\n        info: value.humidity\n    },\n    {\n        selector: '#living-room-temperature-level',\n        attributes: {\n            height: temperatureInPercent(value.temperature[value.temperature.length-1])\n        },\n        animation: _.cloneDeep(defaultAnimation)\n    },\n    {\n        selector: '.therm-colored',\n        attributes: {\n            fill: makeGradientColor(thermCold, thermHot, 100*temperatureInPercent(value.temperature[value.temperature.length-1])).cssColor\n        },\n        animation: _.cloneDeep(defaultAnimation)\n    },\n    {\n        selector: '#living-room-temperature',\n        innerVal: Math.round(value.temperature[value.temperature.length-1]*10)/10\n    },\n    {\n        selector: '#sun-value',\n        innerVal: Math.round(_.last(value.sun))\n    }\n];\n\n\n\nAPI.createData('svgmodifier', svgmodifier);\n});\n\n"
