			"url": "modules/types/edition/filter_editor/",
			"configuration": {
				"sections": {},
				"groups": {
					"group": [
						{
							"script": [
								"//When the result is ready, use resolve(result) to send it.\n//In case of an error, use reject(error)\nvar interval = 1;\nvar tmax = 30;\nvar x = DataObject.recursiveTransform({\n    temperature: [],\n    humidity: [],\n    pressure: [],\n    sun: [],\n});\n\nconsole.log(x, x.temperature._dataChange, x.humidity._dataChange, x.pressure._dataChange, x.sun._dataChange);\n\nfunction getTemperature(t) {\n    return 27+3*Math.cos(t*4*Math.PI/(tmax*1000))\n}\n\nfunction getPressure(t) {\n    return 1010+30*Math.cos(t*4*Math.PI/(tmax*1000));\n}\n\nfunction getHumidity(t) {\n    return 50+20*Math.cos(t*4*Math.PI/(tmax*1000));\n}\n\nfunction getSun(t) {\n    return 500+400*Math.cos(t*4*Math.PI/(tmax*1000));\n}\n\nfunction send(t) {\n    setTimeout(function() {\n        x.temperature.push(getTemperature(t));\n        x.pressure.push(getPressure(t));\n        x.humidity.push(getHumidity(t));\n        x.sun.push(getSun(t));\n        \n        x.pressure.triggerChange();\n        x.temperature.triggerChange();\n        x.humidity.triggerChange();\n        x.sun.triggerChange();\n        \n       API.createData('filterObj', x);\n       //x.triggerChange(false, ['abcd']);\n    }, t);\n}\n\nfor(var i=0; i<=tmax*1000; i+=interval*1000) {\n    send(i);\n}\n\n"
							]
						}
					],
